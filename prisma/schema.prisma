generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tasks {
  taskCompleted Boolean? @default(false)
  taskName      String?
  xata_createdat DateTime @default(now())
  xata_id       String   @id @default(dbgenerated("('rec_'::text || (xata_private.xid())::text)")) @unique
  xata_updatedat DateTime @default(now())
  xata_version  Int      @default(0)
}
